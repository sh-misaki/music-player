{
  "presets": [
    "next/babel",
    "@zeit/next-typescript/babel"
  ],
  "plugins": [
    [
      "styled-components",
      {
        "ssr": true,
        "displayName": true,
        "preprocess": false
      },
    ],
    ["module:babel-root-import", {
      "rootPathSuffix": "src"
    }],
    ["inline-import", { "extensions": [".css"] } ],
  ]
}
